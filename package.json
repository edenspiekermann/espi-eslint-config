{
  "name": "@edenspiekermann/eslint-config",
  "version": "1.2.1",
  "description": "A shareable eslint config for ESPI projects",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint . --ext .js",
    "test": "jest --verbose",
    "release": "np"
  },
  "keywords": [
    "eslint",
    "eslint config",
    "prettier",
    "linting",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edenspiekermann/espi-eslint-config.git"
  },
  "author": "EdenSpiekermann",
  "license": "MIT",
  "homepage": "https://github.com/edenspiekermann/espi-eslint-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.js,!__fixtures/**/*.js": [
      "eslint --fix"
    ]
  },
  "np": {
    "tests": "npm run lint"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-eslint": "^10.0.3",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "^2.3.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.5",
    "eslint": "^7.26.0",
    "husky": "^4.3.8",
    "jest": "^27.0.6",
    "lint-staged": "^10.4.0",
    "np": "^7.4.0",
    "react": "^17.0.2",
    "typescript": "^4.2.4"
  }
}
