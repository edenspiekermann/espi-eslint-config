{
  "name": "@edenspiekermann/eslint-config",
  "version": "1.0.0",
  "description": "A shareable eslint config for ESPI projects",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint . --ext .js",
    "release": "np"
  },
  "keywords": [
    "eslint",
    "eslint config",
    "prettier",
    "linting",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edenspiekermann/espi-eslint-config.git"
  },
  "author": "EdenSpiekermann",
  "license": "MIT",
  "homepage": "https://github.com/edenspiekermann/espi-eslint-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "np": {
    "tests": "npm run lint"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "eslint": ">=6"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^2.5.0",
    "babel-eslint": "^10.0.3",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prettier": "^1.18.2"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "np": "^5.1.1"
  }
}
